{
  "name": "eslint-plugin-cubejs",
  "version": "0.0.0-development",
  "description": "A plugin to enable ESLint (https://eslint.org) for CubeJS (https://cube.dev) projects",
  "keywords": [
    "cube",
    "cube.js",
    "cubejs",
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Morgan Benton <morgan.benton@gmail.com> (https://morphatic.com)",
  "main": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "babel lib --out-dir dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "semantic-release": "semantic-release",
    "test": "mocha tests --recursive"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@semantic-release/changelog": "^5.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "semantic-release": "^17.4.3"
  },
  "peerDependencies": {
    "eslint": ">=7.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "homepage": "https://github.com/morphatic/eslint-plugin-cubejs",
  "repository": {
    "type": "git",
    "url": "https://github.com/morphatic/eslint-plugin-cubejs.git"
  },
  "bugs": {
    "url": "https://github.com/morphatic/eslint-plugin-cubejs/issues"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
